{
  "compilerOptions": {
    "outDir": "./built",
    "allowJs": true,
    "target": "es5",
    "jsx": "react-jsx",
    "esModuleInterop": true,

    "noImplicitAny": true,
    "strictNullChecks": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "sourceMap": true
  },
  "include": ["./src/**/*"]
}
